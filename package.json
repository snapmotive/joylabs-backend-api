{
  "name": "joylabs-backend-api",
  "version": "1.0.0",
  "description": "Serverless backend API for JoyLabs Catalogue App",
  "main": "src/index.js",
  "scripts": {
    "start": "serverless offline start",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "setup-layers": "./scripts/setup-layers.sh",
    "deploy:dev": "npm run setup-layers && serverless deploy --stage dev",
    "deploy:prod": "npm run setup-layers && serverless deploy --stage production",
    "migrate:data": "node scripts/migrate-tables-to-v3.js",
    "update-square-secrets": "node scripts/update-square-secrets.js",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage production",
    "logs:dev": "serverless logs -f api -t --stage dev",
    "logs:prod": "serverless logs -f api -t --stage production",
    "setup-env": "node setup-env.js",
    "test:oauth": "./test-oauth-local.sh",
    "test:square-config": "node test-square-oauth.js",
    "rotate-credentials": "node scripts/rotate-credentials.js",
    "security-check": "npm audit",
    "setup-ssm": "./scripts/add-ssm-params.sh",
    "aws:square-setup": "./scripts/aws-square-setup.sh",
    "aws:check-params": "./scripts/check-aws-params.sh"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "api"
  ],
  "author": "JoyLabs",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1499.0",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "connect-dynamodb": "^3.0.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "expo-crypto": "^14.0.2",
    "expo-linking": "~6.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "serverless-http": "^3.2.0",
    "square": "^33.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-loader": "^10.0.0",
    "chalk": "^5.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "serverless": "^3.38.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^13.3.0",
    "serverless-plugin-include-dependencies": "^6.1.1",
    "serverless-webpack": "^5.15.1",
    "webpack": "^5.98.0",
    "webpack-node-externals": "^3.0.0"
  }
}
