# Environment
NODE_ENV=development

# Server Configuration
PORT=5000
IS_OFFLINE=true
API_BASE_URL=https://ux8uq7hd24.execute-api.us-west-1.amazonaws.com/dev

# AWS Configuration
REGION=us-west-1
# AWS_PROFILE=default

# DynamoDB Tables
PRODUCTS_TABLE=joylabs-catalog-products-v3-dev
CATEGORIES_TABLE=joylabs-catalog-categories-v3-dev
USERS_TABLE=joylabs-catalog-users-v3-dev
WEBHOOKS_TABLE=joylabs-catalog-webhooks-v3-dev
SESSIONS_TABLE=joylabs-sessions-v3-dev
MERCHANT_TABLE=joylabs-backend-api-merchants-v3-dev
WEBHOOK_TABLE=joylabs-backend-api-webhooks-v3-dev

# Authentication
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info

# API Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Square API Configuration
SQUARE_APPLICATION_ID=sq0idp-your-app-id
SQUARE_APPLICATION_SECRET=your-application-secret
SQUARE_WEBHOOK_SIGNATURE_KEY=your-webhook-signature-key
SQUARE_ENVIRONMENT=sandbox # or production

# API Configuration
SQUARE_REDIRECT_URL=https://ux8uq7hd24.execute-api.us-west-1.amazonaws.com/dev/api/auth/square/callback

# Secrets
SESSION_SECRET=your-session-secret 